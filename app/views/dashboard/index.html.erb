
<div class="container">
 <div class="row">
   <div class ="col s5">
     <%= image_tag(@user.image_url, :class=> 'profile-pic') %>
     <h5 class='name'><%= current_user.name %></h5>
     <h5 class='username'><%= current_user.username%></h5>
   </div>
   <div class="col s2">
     </br>
     <h6>Stars <span class="count"><%= @user.starred(current_user.token).count %></span></h6>
   </div>
   <div class="col s2">
     </br>
     <h6>Followers <span class="count"><%= @user.followers_count %></span></h6>
   </div>
   <div class="col s2">
     </br>
     <h6>Following <span class="count"><%= @user.following_count %></span></h6>
   </div>

     <!-- Followers:
     <ul>
       <%# @user.followers(current_user.token).each do |user| %>
        <li> <%#= user %></li>
       <%# end %>
     </ul> -->
   </div>
 </div>
</div>
 